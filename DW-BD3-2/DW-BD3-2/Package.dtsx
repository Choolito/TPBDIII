                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    V.Id_vendedor,
    V.Nombre AS nombre, 
    V.Especialidad AS especialidad, 
    DATEDIFF(DAY, E.fecha_ingreso, GETDATE()) AS antiguedad,
    E.sueldo AS sueldo, 
    ISNULL(SUM(C.duracion_horas), 0) AS cantidad_horas
FROM 
    FacturacionVentas.dbo.Vendedores V
JOIN
    RecursosHumanos.dbo.Empleado E ON V.Id_vendedor = E.legajo
LEFT JOIN
    RecursosHumanos.dbo.CapacitacionXEmpleado CxE ON E.legajo = CxE.legajo
LEFT JOIN
    RecursosHumanos.dbo.Capacitacion C ON CxE.id_capacitacion = C.id_capacitacion
GROUP BY 
    V.Id_vendedor,
    V.Nombre, 
    V.Especialidad, 
    CONVERT(date, E.fecha_ingreso), 
    E.sueldo;</property>
